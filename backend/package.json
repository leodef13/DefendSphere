  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.614.0",
    "@prisma/client": "^5.20.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.11.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "morgan": "^1.10.0",
