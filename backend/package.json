{
  "name": "defendsphere-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "DefendSphere Security Dashboard Backend API",
  "main": "index.js",
  "scripts": {
    "prestart": "prisma generate",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "worker:parse": "node workers/parseWorker.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "db:seed": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/seed.ts",
